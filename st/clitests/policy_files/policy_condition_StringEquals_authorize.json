{
  "Id": "Policy1462526893193",
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Stmt1462526862401",
      "Action": [
        "s3:PutObject"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::seagate/*",
      "Condition": {
        "StringEquals": {
          "s3:x-amz-acl": "bucket-owner-read"
        }
      },
      "Principal":"*"
    }
  ]
}
